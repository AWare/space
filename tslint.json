{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "jsx-no-lambda": false,
    "no-console": false,
    "interface-name": false,
    "import-name": false,
    "no-debugger": false,
    "variable-name": [true, "allow-pascal-case", "allow-trailing-underscore"]
  }
}
